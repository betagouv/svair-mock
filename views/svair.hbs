<div class="titre_affiche_avis">
<span>Impôt   {{ anneeImpots }} sur les revenus de l'année  {{ anneeRevenus }}
</span>
</div>


			<table>
				<tbody>
				<tr>
					<td class="label">
					</td>
					<td class="label">Déclarant 1
					</td>
					<td class="label">Déclarant 2
					</td>
				</tr>
				<tr>
					<td class="labelPair">Nom
					</td>
					<td class="labelPair">{{{ declarant1.nom }}}
					</td>
					<td class="labelPair">{{{ declarant2.nom }}}
					</td>
				</tr>
				<tr>
					<td class="labelImpair">Nom de naissance
					</td>
					<td class="labelImpair">{{{ declarant1.nomNaissance }}}
					</td>
					<td class="labelImpair">{{{ declarant2.nomNaissance }}}
					</td>
				</tr>
				<tr>
					<td class="labelPair">Prénom(s)
					</td>
					<td class="labelPair">{{{ declarant1.prenoms }}}
					</td>
					<td class="labelPair">{{{ declarant2.prenoms }}}
					</td>
				</tr>
				<tr>
					<td class="labelImpair">Date de naissance
					</td>
					<td class="labelImpair">{{{ declarant1.dob }}}
					</td>
					<td class="labelImpair">{{{ declarant2.dob }}}
					</td>
				</tr>
				<tr>
					<td class="labelPair">
						Adresse déclarée au 1<sup>er</sup> janvier  {{ anneeImpots }}
					</td>
					<td class="labelPair">{{{ adress.ligne1 }}}
					</td>
					<td class="labelPair">
					</td>
				</tr>
					<tr>
					<td class="labelPair">
					</td>
					<td colspan="2" class="labelPair">{{{ adress.ligne2 }}}
					</td>
					</tr>
				<tr>
					<td class="espace"></td>
				</tr>
				<tr>
					<td class="labelPair" colspan="2">Date de mise en recouvrement de l'avis d'impôt
					</td>
					<td class="textPair">{{{ dateRecouvrement }}}
					</td>
				</tr>
				<tr>
					<td class="labelImpair" colspan="2">Date d'établissement
					</td>
					<td class="textImpair">{{{ dateEtablissement }}}
					</td>
				</tr>
				<tr>
					<td class="labelPair" colspan="2">Nombre de part(s)
					</td>
					<td class="textPair">{{{ nombreParts }}}
					</td>
				</tr>
				<tr>
					<td class="labelImpair" colspan="2">Situation de famille
					</td>
					<td class="textImpair">{{{ situationFamiliale }}}
					</td>
				</tr>
				<tr>
					<td class="labelPair" colspan="2">Nombre de personne(s) à charge
					</td>
					<td class="textPair">{{{ nombreDePersonneACharge }}}
					</td>
				</tr>
				<tr>
					<td class="labelImpair" colspan="2">Revenu brut global
					</td>
					<td class="textImpair">{{{ revenuBrutGlobal }}}
					</td>
				</tr>
				<tr>
					<td class="labelPair" colspan="2">Revenu imposable
					</td>
					<td class="textPair">{{{ revenuImposable }}}
					</td>
				</tr>
				<tr>
					<td class="labelImpair" colspan="2">Impôt sur le revenu net avant corrections
					</td>
					<td class="textImpair">{{{ impotsNetAvantCorrections }}}
					</td>
				</tr>
				<tr>
					<td class="labelPair" colspan="2">Montant de l'impôt
					</td>
					<td class="textPair">{{{ impots }}}
					</td>
				</tr>
				<tr>
					<td class="labelImpair" colspan="2">Revenu fiscal de référence
					</td>
					<td class="textImpair">{{{ revenuFiscalReference }}}
					</td>
				</tr>

			</tbody></table>

			<div id="situationPartielle">{{{ situationPartielle }}}
			</div>

			{{#if erreurCorrectif}}
			<div id="erreurCorrectif">{{{ erreurCorrectif }}}</div>
			{{/if}}

			<div id="boutonsAvis">
				<a class="nouvelle_recherche" href="/secavis/">
					Nouvelle recherche
				</a>
			 </div>
